<% if(errors.length > 0) { %>
    <div class="errors">
        <h1>
            <% errors.forEach(erro =>{ %>
                    <%= erro %></br>
                <% }); %>
        </h1>
    </div>
<% } %>
<% if(sucess.length > 0) { %>
    <div class="sucess">
        <h1>
            <% sucess.forEach(sucess =>{ %>
                    <%= sucess %></br>
                <% }); %>
        </h1>
    </div>
<% } %>